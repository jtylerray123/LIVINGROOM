{
  "name": "livingroom_games",
  "version": "1.0.0",
  "description": "A mobile-first web application for playing various multiplayer games with friends and family, whether physically present or connected via video call.",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "server": "ts-node-dev --respawn --transpile-only server/index.ts",
    "start": "concurrently \"npm run dev\" \"npm run server\""
  },
  "keywords": [
    "games",
    "multiplayer",
    "web-app",
    "react"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.10.0",
    "@reduxjs/toolkit": "^2.0.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.20.0",
    "redis": "^4.6.10",
    "socket.io": "^4.7.2",
    "tailwindcss": "^3.3.5"
  },
  "devDependencies": {
    "@types/node": "^20.9.2",
    "@types/react": "^18.2.37",
    "@types/react-dom": "^18.2.15",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.2",
    "postcss": "^8.5.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0"
  }
}
